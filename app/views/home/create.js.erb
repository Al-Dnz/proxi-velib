 var coord = [ <%= @placefound.latitude %>, <%= @placefound.longitude %>];

 var placefound = {latitude: <%= @placefound.latitude %> , longitude: <%= @placefound.longitude %>  };
 var placefound_name = "<%= @placefound_name %>" ;

function findPlace()
{
  console.log("coord => "+coord);
  document.getElementById('table-title').innerHTML= "STATIONS À PROXIMITÉ DE "+ placefound_name;
  document.getElementById('table-content').innerHTML= "";
  centerPoint = coord;
  init(placefound);
  map.off();
  map.remove();

  thpMarker = false ;
  createMap(coord, thpMarker);
  setMainMarker();

}

findPlace() ;

